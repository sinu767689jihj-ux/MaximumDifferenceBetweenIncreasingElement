public class MaximumDifferenceBetweenIncreasingElementUsingBruteForce {
    public int maximum(int[] arr)
    {
        int max=0;
        for(int i=0;i<arr.length-1;i++)
        {
            for(int j=i+1;j<arr.length;j++)
            {
                int a=arr[i];
                int b=arr[j];
                if(a<b)
                {
                    int temp=arr[j]-arr[i];
                    max=Math.max(temp,max);
                }
            }
        }
        return max;
    }
    public static void main(String[] args)
    {
        int[] arr={9,2,3,4,10};
        MaximumDifferenceBetweenIncreasingElementUsingBruteForce max=new MaximumDifferenceBetweenIncreasingElementUsingBruteForce();
        System.out.println(max.maximum(arr));
    }
}
//Time Complexity:O(n^2)
